    "eslint-plugin-vue": "^7.8.0",
    "ora": "^5.4.0",
    "prettier": "^2.2.1",
    "rollup-plugin-esbuild": "^3.0.2",
    "rollup-plugin-node-externals": "^2.2.0",
    "typescript": "^4.2.3",
    "vite": "^2.0.5",
    "vue-eslint-parser": "^7.6.0",
    "wait-on": "^5.3.0",
    "yargs": "^16.2.0"
  },
